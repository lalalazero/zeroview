{
  "name": "@zeroplayground/vue2-components",
  "version": "0.0.14",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve-example": "cross-env EXAMPLE_ENV=true vue-cli-service serve src/example.js",
    "build": "webpack ",
    "build-example": "cross-env EXAMPLE_ENV=true vue-cli-service build src/example.js",
    "test": "cross-env BABEL_ENV=test karma start --single-run",
    "test-dev": "karma start",
    "analyze": "cross-env ANALYZE=true vue-cli-service build --target lib --name zeroview src/index.js",
    "analyze-example": "cross-env EXAMPLE_ENV=true ANALYZE=true vue-cli-service build src/example.js",
    "webpack-build": "cross-env NODE_ENV=production webpack --config build/webpack.config.js",
    "webpack-build-example": "cross-env EXAMPLE_ENV=true webpack --config build/webpack.config.example.js",
    "new-dev": "webpack serve --config ./example/webpack.config.js",
    "new-dev-build": "webpack --config ./example/webpack.config.js"
  },
  "main": "dist/zeroview.umd.min.js",
  "files": [
    "src/components",
    "src/directives",
    "src/index.js",
    "src/style.scss",
    "vue.config.js",
    "babel.config.js",
    "README.md",
    "dist/zeroview.common.js",
    "dist/zeroview.css",
    "dist/zeroview.umd.js"
  ],
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.7.14",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-unit-mocha": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-loader": "^9.1.3",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.1.2",
    "cross-env": "^6.0.3",
    "css-loader": "^6.8.1",
    "highlight.js": "^9.16.2",
    "html-webpack-plugin": "^5.5.3",
    "karma": "^4.2.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.1",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^4.0.2",
    "markdown-it": "^10.0.0",
    "markdown-it-anchor": "^5.2.5",
    "markdown-it-chain": "^1.3.0",
    "markdown-it-container": "^2.0.0",
    "mini-css-extract-plugin": "^2.7.6",
    "mocha": "^6.2.0",
    "postcss-loader": "^7.3.3",
    "sass": "^1.64.2",
    "sass-loader": "^7.3.1",
    "sinon": "^7.4.1",
    "sinon-chai": "^3.3.0",
    "style-resources-loader": "^1.2.1",
    "transliteration": "^2.1.7",
    "vue-cli-plugin-style-resources-loader": "^0.1.3",
    "vue-loader": "^15.10.1",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.7.14",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
